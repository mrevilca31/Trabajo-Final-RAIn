<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.idexadorrain.FileTypeSelectionController" alignment="CENTER" spacing="20">
    <ComboBox fx:id="fileTypeComboBox" promptText="Selecccione el tipo de archivo a indexar">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="PDF"/>
                <String fx:value="WORD"/>
                <String fx:value="EXCEL"/>
            </FXCollections>
        </items>
    </ComboBox>
    <Button text="Continuar" onAction="#handleContinue"/>
</VBox>

