<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<VBox xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.idexadorrain.FileTypeSelectionController" alignment="CENTER" spacing="20">
    <ComboBox fx:id="fileTypeComboBox" promptText="Select file type">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="PDF"/>
                <String fx:value="Word"/>
                <String fx:value="Excel"/>
            </FXCollections>
        </items>
    </ComboBox>
    <Button text="Continue" onAction="#handleContinue"/>
</VBox>

